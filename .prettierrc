{
	"semi": false,
	"tabWidth": 2,
	"useTabs": true,
	"printWidth": 135,
	"bracketSpacing": true,
	"arrowParens": "always",
	"trailingComma": "all",
	"singleQuote": true,
	"endOfLine": "lf",
	"jsxSingleQuote": true,
	"jsxBracketSameLine": false,
	"bracketSameLine": false,
	"plugins": [
		"prettier-plugin-tailwindcss"
	]
}
